<%= form_for @artist do |f| %>
  <fieldset>
    <%= f.label :name %>
    <%= f.text_field :name %> <!-- gives us <input type="text" name="title">  -->
  </fieldset>
  <fieldset>
    <%= f.label :nationality %>
    <%= f.text_field :nationality %>
  </fieldset>
  <fieldset>
    <%= f.label :dob %>
    <%= f.date_field :dob %>
  </fieldset>
  <fieldset>
    <%= f.label :period %>
    <%= f.text_field :period %>
  </fieldset>

  <fieldset>
    <%= f.label :roundness %>
    <%= f.text_field :roundness %>
  </fieldset>

  <fieldset>
    <%= f.label :image, 'Image URL' %>
    <%= f.url_field :image %>
  </fieldset>

  <%= f.submit %>

<% end %>
